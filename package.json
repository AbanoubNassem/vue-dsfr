{
  "name": "@laruiss/vue-dsfr",
  "version": "1.0.0-beta.35",
  "files": [
    "dist",
    "types",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": "17.x.x",
    "npm": "8.x.x"
  },
  "types": "./types",
  "license": "MIT",
  "main": "./dist/vue-dsfr.umd.js",
  "module": "./dist/vue-dsfr.esm.js",
  "publishConfig": {
    "@laruiss:registry": "https://registry.npmjs.com",
    "access": "public"
  },
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "check-exports": "node ./ci/check-exports.mjs",
    "check-exports-ci": "node ./ci/check-exports.mjs --ci",
    "dev": "npm run storybook",
    "clean-dist": "rimraf dist",
    "prebuild": "run-s clean-dist typegen",
    "build": "run-p build:css build:js",
    "postbuild": "copyfiles types/composable.d.ts dist/composable.d.ts",
    "build:css": "node build-css.mjs",
    "build:js": "cross-env NODE_ENV=production rollup --config rollup.config.js",
    "demo-app": "vite",
    "test": "run-s test:unit test:ct-ci",
    "test:unit": "jest",
    "test:ct": "cypress open-ct",
    "test:ct-ci": "cypress run-ct",
    "format": "npm run lint -- --fix",
    "lint": "eslint ./src/**/*.{vue,js}",
    "storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "vite:build": "vite build && tsc -d --emitDeclarationOnly && vue-typegen gen -s src -o types",
    "typegen": "tsc -d --emitDeclarationOnly && vue-typegen gen -s src -o types",
    "vue-tsc": "vue-tsc --declaration --emitDeclarationOnly"
  },
  "dependencies": {
    "@gouvfr/dsfr": "^1.3.1",
    "focus-trap": "^6.7.3",
    "focus-trap-vue": "^3.2.1",
    "oh-vue-icons": "^0.4.7",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-syntax-jsx": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@cypress/vite-dev-server": "^2.2.2",
    "@cypress/vue": "^3.1.1",
    "@rushstack/eslint-patch": "^1.1.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-url": "^6.1.0",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-docs": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/client-api": "^6.4.9",
    "@storybook/core-common": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@storybook/vue3": "^6.4.9",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/user-event": "^13.5.0",
    "@testing-library/vue": "^6.5.1",
    "@types/jsdom": "^16.2.14",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.25",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/tsconfig": "^0.1.3",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-import-meta": "^2.1.1",
    "chalk": "^5.0.0",
    "copyfiles": "^2.4.1",
    "core-js": "^3.21.1",
    "cross-env": "^7.0.3",
    "cypress": "^9.5.1",
    "cypress-plugin-tab": "^1.0.5",
    "eslint": "^8.10.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-storybook": "^0.5.7",
    "eslint-plugin-vue": "^8.5.0",
    "globby": "^13.1.1",
    "inquirer": "^8.2.0",
    "jest": "^27.5.1",
    "jest-svg-transformer": "^1.0.0",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.4",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.7",
    "postcss-csso": "^6.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.4.1",
    "postcss-url": "^10.1.3",
    "rimraf": "^3.0.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "rollup-plugin-vue": "^6.0.0",
    "semantic-release": "^19.0.2",
    "start-server-and-test": "^1.14.0",
    "storybook-builder-vite": "^0.1.17",
    "ts-jest": "^27.1.3",
    "tsd-jsdoc": "^2.5.0",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vitest": "^0.5.0",
    "vue-loader": "^17.0.0",
    "vue-router": "^4.0.13",
    "vue-router-mock": "^0.1.7",
    "vue-tsc": "^0.32.1",
    "vue-typegen": "^0.2.0",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "vue-router": "^4.0.13"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "pre-push": "npm test"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": "run-s check-exports-ci lint --"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/laruiss/vue-dsfr.git"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true,
        "branch": "develop"
      }
    ],
    "assets": [
      "dist/*.js",
      "dist/*.css"
    ]
  }
}
