{
  "name": "@gouvminint/vue-dsfr",
  "version": "3.6.0",
  "files": [
    "dist",
    "lib",
    "types",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=18.x.x",
    "npm": ">=8.x.x"
  },
  "types": "./types",
  "license": "MIT",
  "main": "./dist/vue-dsfr.umd.js",
  "module": "./dist/vue-dsfr.mjs",
  "exports": {
    ".": {
      "require": {
        "types": "./types/index.d.ts",
        "default": "./dist/vue-dsfr.umd.js"
      },
      "import": {
        "types": "./types/index.d.ts",
        "default": "./dist/vue-dsfr.mjs"
      }
    },
    "./dist/vue-dsfr.css": {
      "require": "./dist/vue-dsfr.css",
      "import": "./dist/vue-dsfr.css"
    },
    "./styles": {
      "require": "./dist/vue-dsfr.css",
      "import": "./dist/vue-dsfr.css"
    }
  },
  "publishConfig": {
    "@gouvminint:registry": "https://registry.npmjs.com",
    "access": "public"
  },
  "scripts": {
    "check-exports": "node ./ci/check-exports.mjs",
    "check-exports-ci": "node ./ci/check-exports.mjs --ci",
    "clean-dist": "rimraf dist lib",
    "prebuild": "run-s clean-dist",
    "build": "run-s build:vite typegen",
    "build:vite": "vite build",
    "demo": "vite",
    "dev": "storybook dev -p 6006",
    "vitest": "vitest",
    "coverage": "vitest run --coverage",
    "test": "run-s test:unit test:ct-ci",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run",
    "test:ct": "cypress open --component",
    "test:ct-ci": "cypress run --component",
    "format": "run-s lint -- --fix",
    "lint": "eslint ./src/**/*.{vue,js,ts}",
    "lint:staged": "eslint",
    "prepublishOnly": "run-s build",
    "semantic-release": "semantic-release",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "vite:build": "vite build && npm run typegen",
    "typegen": "vue-tsc --declaration --emitDeclarationOnly",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@gouvfr/dsfr": "~1.10.2",
    "focus-trap": "^7.5.4",
    "focus-trap-vue": "^4.0.3",
    "oh-vue-icons": "1.0.0-rc3",
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^5.0.6",
    "@cypress/vue": "^6.0.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@storybook/addon-a11y": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/addon-themes": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/test": "^7.6.0-alpha.4",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/vue3": "^7.5.3",
    "@storybook/vue3-vite": "^7.5.3",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^7.0.0",
    "@types/jsdom": "^21.1.2",
    "@types/node": "^18.17.14",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/compiler-sfc": "^3.3.8",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-import-meta": "^2.2.1",
    "browserslist": "^4.22.1",
    "c8": "^8.0.1",
    "chalk": "^5.3.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "cypress": "^13.5.0",
    "cypress-plugin-tab": "^1.0.5",
    "eslint": "^8.53.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vue": "^9.18.1",
    "globby": "^13.2.2",
    "inquirer": "^9.2.12",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "mkdirp": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "p-debounce": "^4.0.0",
    "postcss": "^8.4.31",
    "postcss-csso": "^6.0.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.3.0",
    "postcss-url": "^10.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.9.2",
    "semantic-release": "^21.1.1",
    "start-server-and-test": "^2.0.0",
    "storybook": "^7.5.3",
    "tsd-jsdoc": "^2.5.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "vue-eslint-parser": "^9.3.2",
    "vue-loader": "^17.3.1",
    "vue-router": "^4.2.5",
    "vue-router-mock": "^1.0.0",
    "vue-tsc": "^1.8.22",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "oh-vue-icons": "1.0.0-rc3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "optionalDependencies": {
    "esbuild-linux-64": "^0.15.18"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "pre-push": "npm test"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": "run-s check-exports-ci lint --"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dnum-mi/vue-dsfr.git"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true,
        "channel": "beta"
      },
      {
        "name": "next",
        "prerelease": true,
        "channel": "next"
      }
    ],
    "assets": [
      "dist/*.js",
      "dist/*.css"
    ]
  },
  "volta": {
    "node": "18.17.1"
  }
}
